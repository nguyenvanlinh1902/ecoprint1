{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@contexts/*": ["src/contexts/*"],
      "@hooks/*": ["src/hooks/*"],
      "@services/*": ["src/services/*"],
      "@config/*": ["src/config/*"],
      "@styles/*": ["src/styles/*"],
      "@const/*": ["src/const/*"],
      "@resources/*": ["src/resources/*"]
    }
  },
  "include": ["src/**/*"]
} 