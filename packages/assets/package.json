{
  "name": "b2b-platform-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "build:analyze": "NODE_ENV=production ANALYZE=true vite build --mode production",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "preview:prod": "vite preview --mode production",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy:dev": "npm run build:dev && firebase deploy --only hosting -P dev",
    "deploy:staging": "npm run build:staging && firebase deploy --only hosting -P default",
    "deploy:prod": "npm run build:prod && firebase deploy --only hosting -P default"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.129",
    "@mui/material": "^5.13.0",
    "@mui/x-data-grid": "^6.3.0",
    "@mui/x-date-pickers": "^6.3.0",
    "@shopify/polaris": "^13.9.4",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "firebase": "^11.6.0",
    "form-data": "^4.0.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "prop-types": "^15.8.1",
    "query-string": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.11.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.5.0",
    "uuid": "^11.1.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.0.0",
    "sass": "^1.62.1",
    "vite": "^4.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^0.31.0"
  }
}
